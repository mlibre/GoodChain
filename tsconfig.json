{
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		// "module": "commonjs",
		// "module": "ESNext",
		// "moduleResolution": "node",
		"rootDir": "./src",
		"outDir": "./dist",
		"esModuleInterop": true,
		"importHelpers": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"useUnknownInCatchVariables": true,
		"noUnusedLocals": true,
		// "isolatedModules": true
	},
	// "include": [
	// 	"**/*.ts",
	// 	"**/*.tsx",
	// 	"src/**/*"
	// ]
}